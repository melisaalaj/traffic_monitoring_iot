# Ignore all files ending with .log
*.log

# Ignore a specific file named "config.local.js"
config.local.js

# Ignore the entire "node_modules" directory
node_modules/

# Ignore the "build" directory and its contents
build/

# Ignore all files in the "temp" directory but not the directory itself
temp/*

# Exclude a specific file from an ignored directory
# (e.g., ignore all .tmp files, but not "important.tmp" in the "docs" directory)
docs/*.tmp
!docs/important.tmp

# Ignore all files with the .DS_Store extension (macOS specific)
.DS_Store

# Ignore all files generated by common IDEs
.idea/
.vscode/

# Ignore generated executables
*.exe
*.out
__pycache__/
*.jar
.env
